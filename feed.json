			"channel": "GitHub中文社区",
			"url": "https://www.githubs.cn/api/graphql",
			"_comment": "https://www.githubs.cn/trending",
			"enable": 1,
			"category": "日报",
			"tags": ["GitHub", "日报", "Daily", "科技", "Tech"],
			"request": {
				"method": "post",
				"payload": {
					"query": "query trendingQuery(\n  $locale: String\n  $lang: String\n) {\n  dailyTrends: trending(range: \"daily\", locale: $locale, lang: $lang) {\n    ...Trends_trends\n  }\n  weeklyTrends: trending(range: \"weekly\", locale: $locale, lang: $lang) {\n    ...Trends_trends\n  }\n  monthlyTrends: trending(range: \"monthly\", locale: $locale, lang: $lang) {\n    ...Trends_trends\n  }\n}\n\nfragment Trends_trends on Trends {\n  dateRange\n  issueId\n  updateAt\n  projects {\n    id\n    name\n    fullName\n    sourceUrl\n    starCount\n    forkCount\n    description\n    remark\n    lang\n    screenshot\n    url\n    homepageUrl\n    descTrans\n    category(locale: $locale) {\n      name\n      screenName\n      id\n    }\n    tags {\n      name\n      id\n    }\n    createAt\n    updateAt\n    user {\n      name\n      remark\n      poweredBy\n      id\n    }\n    wiki\n    chinaFlag\n    commentCount\n  }\n}\n",
					"variables": {
						"locale": "zh",
						"lang": null
					}
				}
			},
			"response": {
				"list": "data.dailyTrends.projects"
			},
			"entry": {
				"date": "updateAt",
				"title": "name",
